{
  "manifest_version": 2,
  "name": "History Music",
  "description": "Turn your search history into a tune",
  "version": "0.1.0",
  "permissions": [
    "storage",
    "tabs",
    "activeTab"
  ],
  "browser_action": {
    "default_title": "Musical Search"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
